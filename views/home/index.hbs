<div class="container body-content">
    <div class="row">
        <div class="col-sm-12">
            <a class="banner" href="https://softuni.bg/">
                <img
                        src="data:image/png;base64,"
                        alt="logo"
                        width="1350"

                />
            </a>
        </div>
    </div>
    {{#each articles}}
        <div class="col-md-4">
            <article>
                <header>
                    <h2>
                        <a href="/article/details/{{this.id}}">
                            {{this.title}}
                        </a>
                    </h2>
                </header>

                <small class="author" >Автор:
                    {{this.author.fullName}}
                </small>

                <p>{{this.content}}
                </p>

                <footer>
                    <div class="pull-right">
                        <a class="read-more" href="/article/details/{{this.id}}">Прочети... &raquo;</a>
                    </div>
                </footer>
            </article>
        </div>
    {{/each}}
</div>

